     internal_properties : CAAAAAgAAABKpVYgM57zySR0m7lGWANaleLOIrbVkbZ+QIAZSd4yNwCdKzpJONTp7paC7e6WNY5s3Lz1No41UxiUrS6kZIw9N4h6omxKb2oGXuooipu0p7QJ54/7XOH+iifCPPESbNFhQNddoteDvM7pt4gJAQz1Glhu2/VeOV4Rzm2Z/hb2J7MNNacEh7YTyabIMBWRdGWn5Hd2qSI=
     procedure_id : 2255441125620642730
     type_code : 14
     code : |1-
      // Résumé : Renvoyer le chemin de la dernière photo prise
      // Syntaxe :
      //[ <Résultat> = ] DernièrePhoto ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	chaîne : Le chemin de la dernière photo prise
      //
      PROCEDURE DernièrePhoto()
      
      sImage est une chaine
      sListeFichier est une chaine
      
      sListeFichier = fListeFichier( gsRépertoire + [ "/" ] +  "*.jpg", frNonRécursif )
      
      // Pour chaque fichier trouvé
      POUR TOUTE CHAINE UnFichier DE sListeFichier SEPAREE PAR RC
      	// Si c'est l'image est plus récente on la garde
      	Si sImage = "" _OU_ UnFichier > sImage ALORS
      		sImage = UnFichier
      	FIN
      FIN
      
      RENVOYER sImage
     type : 458752
