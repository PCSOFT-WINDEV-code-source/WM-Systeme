   -
     name : DéclencheEcritureNFC
     internal_properties : CAAAAAgAAADSc+1l27zILXZ2NzCfddv2OGWqZdoHudA1z1rEuBUgjz7X7TGnY6kYueozYUgcrsmkf7EQitzf4n3t0izpwH9yF00FKpqYTUw1NhSXLWAlqnGOzVSXGq3uLE777C04RYyR7vyv1JEYRYqe9u0J
     procedure_id : 1271755329785066821
     type_code : 14
     code : |1-
      procédure DéclencheEcritureNFC()
      
      	// Ecrit une url dans un tag NFC
      	tag	est un nfcTag
      	url	est une nfcDonnée
      	
      	url..Type		= nfcURI
      	url..Contenu	= "https://www.pcsoft.fr"
      	
      	// Ajout de la donnée dans le tag
      	Ajoute( tag..Donnée, url )
      	
      	// Écriture NFC
      	NFCEcritTag( tag, procédure( nRes est un entier ) {
      		
      		SI nRes <> nfcOK ALORS
      			ErreurAsynchrone( "Échec de l’écriture sur le tag NFC. ", ErreurInfo() )
      		SINON
      			ToastAffiche( "Écriture NFC réalisée avec succès." )
      		FIN
      	} )
     type : 458752
   -
     name : DéclencheLectureNFC
     procedure_id : 1271755445749669909
     type_code : 14
     code : |1+
      procédure DéclencheLectureNFC()
      	
      	// Lecture d'un tag NFC
      	NFCLitTag( procédure( tag est un nfcTag ) {
      		
      		<COMPILE SI TypeConfiguration=Android>
      			InfoAsynchrone( ChaîneConstruit( "Lecture d’un tag NFC de type %1", tag.Type ) )		
      		<FIN>
      		
      		POUR TOUTE donnée DE tag.Donnée
      			
      			SELON donnée..Type
      			
      				CAS nfcURI
      					
      					SI ChaîneCommencePar( donnée..Contenu, "http" ) ALORS
      						NavigateurLanceAppli( donnée..Contenu )
      					FIN        
      					
      				AUTRES CAS
      					InfoAsynchrone( donnée..Contenu )                           
      					
      			FIN
      			
      		FIN
      		
      	} )
     type : 458752
