  procedures :
   -
     name : DetectionOrientation
     internal_properties : CAAAAAgAAADn0eYtfZUX8gBbcHz5rZp5+ue/PJ8YvIHPff78nCGJCZei0CT1Lp2dtnUTWuBOM6TFSLgYJh7vI+cOTa6HWTXNxHjxHHgxIWPJq19V5aOfUxQXhQ4NKZ8zqusCvoUFIQu6ZVIfnJruxogq24A=
     procedure_id : 2255463322028265648
     type_code : 14
     code : |1+
      // Résumé : Procédure callback appelée lorsque le téléphone change d'orientation
      // Syntax:
      //DetectionOrientation (<nAzimut> is int, <nPitch> is int, <nRoll> is int)
      //
      // Parameters:
      //	nAzimut (int): est un entier correspondant à l'angle entre la direction du Nord magnétique et la direction de l'axe longitudinal (Y) de l'appareil
      //	nPitch (int): est un entier correspondant à l'angle de rotation de l'appareil autour de l'axe horizontal (X)
      //	nRoll (int): est un entier correspondant à l'angle de rotation de l'appareil autour de l'axe longitudinal (Y).
      
      PROCEDURE DetectionOrientation( nAzimut est un entier, nPitch est un entier <utile>, nRoll est un entier <utile> )
      
      iImageAvecRotation est une image = giImageFleche
      
      // On fait tourner l'aiguille
      dRotation( iImageAvecRotation, 360 - nAzimut )
      
      // On affiche l'aiguille
      IMG_Fleche = iImageAvecRotation
     type : 458752
